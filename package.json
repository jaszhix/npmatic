{
  "name": "npmatic",
  "version": "0.1.0",
  "description": "A front-end for NPM package management, developed with NodeJS and Electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.config.prod.js --progress --profile --bail",
    "pack": "build --dir",
    "dist-linux": "npm run build && build -l",
    "dist-win32": "npm run build && build -w",
    "dist-mac": "npm run build && build -m"
  },
  "build": {
    "appId": "npmatic",
    "asar": true,
    "linux": {
      "category": "System",
      "description": "A front-end for NPM package management.",
      "target": ["deb"],
      "desktop": {
        "type": "Application",
        "encoding": "UTF-8",
        "comment": "A front-end for NPM package management.",
        "exec": "npmatic",
        "icon": "./build/48x48.png"
      },
      "depends": ["libappindicator1", "libnotify-bin"],
      "executableName": "npmatic"
    },
    "dmg": {
      "backgroundColor": "#CB3837",
      "iconSize": 48
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    }
  },
  "directories": {
    "buildResources": "build/",
    "output": "pkgs"
  },
  "keywords": [
    "electron",
    "webpack",
    "react",
    "node",
    "npm",
    "package",
    "manager",
    "gui"
  ],
  "author": {
    "name": "Jason Hicks",
    "email": "jaszhix@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.11",
    "css-loader": "^0.25.0",
    "electron": "^1.4.11",
    "eslint": "^3.8.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.13.3",
    "file-loader": "^0.9.0",
    "hot-reload-server": "0.0.4",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.13",
    "postcss-loader": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "svgo": "^0.7.1",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "all-the-package-names": "^1.3044.0",
    "axios": "^0.15.2",
    "css-loader": "^0.25.0",
    "curlrequest": "^1.0.0",
    "dotenv": "^2.0.0",
    "electron-remote": "^1.0.7",
    "electron-window-state": "^3.1.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "jquery": "^3.1.1",
    "kmp": "0.0.3",
    "lodash": "^4.16.4",
    "node-localstorage": "^1.3.0",
    "npm-registry-package-info": "^1.0.5",
    "npm-registry-packages": "^1.1.0",
    "npm-registry-url": "^1.0.0",
    "object-to-arrays": "^1.0.0",
    "open-external": "^0.1.1",
    "package-json": "^2.4.0",
    "package-stream": "^1.0.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-markdown": "^2.4.2",
    "react-onclickoutside": "^5.7.1",
    "react-utils": "^1.0.0",
    "reflux": "^0.4.1",
    "resolve-dir": "^1.0.0",
    "style-loader": "^0.13.1",
    "sudo-prompt": "^6.2.0",
    "system-install": "^1.0.0",
    "tinycolor2": "^1.4.1",
    "to-space-case": "^1.0.0"
  }
}
